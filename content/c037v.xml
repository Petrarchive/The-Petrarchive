      <body>
         <pb n="charta 37 verso" facs="../images/vat-lat3195-f/vat-lat3195-f-037v.jpg" /> 
          <lg type="sonnet" xml:id="rvf182" n="182"> 
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#red #fs24pt">A</hi><hi rendition="#small-caps">m</hi>or <choice><orig>chencende</orig><reg>che ’ncende</reg></choice> il cor dardente <choice><orig>çelo</orig><reg>zelo</reg></choice><choice><orig> &virg;</orig><reg>,</reg></choice></l> 
                      <l n="2">Di gelata paura il ten constretto<choice><orig> .</orig><reg>;</reg></choice> </l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Et qual sia <choice><orig>piu</orig><reg>più</reg></choice><choice><orig> &virg;</orig><reg>,</reg></choice> fa dubbio <choice><orig>alintellecto</orig><reg>a l’intellecto</reg></choice><choice><orig> &virg;</orig><reg>,</reg></choice></l>
                      <l n="4">La <choice><orig>sperança</orig><reg>speranza</reg></choice><choice><orig> &virg;</orig><reg></reg></choice> <choice><orig>ol</orig><reg>o ’l</reg></choice> temor<choice><orig> &virg-underdot;</orig><reg>,</reg></choice> la fiamma <choice><orig>ol</orig><reg>o ’l</reg></choice> gielo.</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5"><choice><orig>Tremal</orig><reg>Trem’ al</reg></choice> <choice><orig>piu</orig><reg>più</reg></choice> caldo<choice><orig>.</orig><reg>,</reg></choice> <choice><orig>ardal</orig><reg>ard’ al</reg></choice> <choice><orig>piu</orig><reg>più</reg></choice> freddo cielo<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="6"><choice><abbr>Sēpre</abbr><expan>Sempre</expan></choice> pien di desire<choice><orig> &virg;</orig><reg></reg></choice> &amp7; di sospetto<choice><orig>.</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Pur come donna &in; un <choice><orig>uestire</orig><reg>vestire</reg></choice> schietto</l>
                      <l n="8">Celi un huom <choice><orig>uiuo</orig><reg>vivo</reg></choice><choice><orig>.</orig><reg>,</reg></choice> o sotto un picciol <choice><orig>uelo</orig><reg>velo</reg></choice>.</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><hi rendition="#sestet_maker">-</hi> <choice><orig><hi rendition="#sestet_marker">-</hi></orig><reg></reg></choice>Di queste pene <choice><orig>e</orig><reg>è</reg></choice> mia <choice><abbr>&#42835;pia</abbr><expan>propia</expan></choice> la prima<supplied>,</supplied></l>
                      <l n="10">Arder <choice><orig>di</orig><reg>dì</reg></choice> &amp7; notte<choice><orig> .</orig><reg>;</reg></choice> &amp7; &quanto; <choice><orig>el</orig><reg>è ’l</reg></choice> dolce male</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11"><choice><orig>Nen</orig><reg>Né ’n</reg></choice> penser cape<choice><orig> &virg;</orig><reg>,</reg></choice> &non; <choice><orig>chen</orig><reg>che ’n</reg></choice> <choice><orig>uersi</orig><reg>versi</reg></choice> <choice><orig>| </orig><reg></reg></choice><choice><orig>on</orig><reg>o ’n</reg></choice> rima<choice><orig> .</orig><reg>;</reg></choice></l>
                      <l n="12"><choice><orig>Laltra</orig><reg>L’altra</reg></choice> &non; <choice><orig>gia</orig><reg>già</reg></choice><choice><orig> .</orig><reg>,</reg></choice> <choice><orig>chel</orig><reg>ché ’l</reg></choice> mio bel foco <choice><orig>e</orig><reg>è</reg></choice> tale<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13"><choice><orig>Chogniuom</orig><reg>Ch’ogni uom</reg></choice> pareggia<choice><orig> .</orig><reg>; </reg></choice>&amp7; del suo lume &in; cima<choice><orig> .</orig><reg></reg></choice></l>
                      <l n="14">Chi <choice><orig>uolar</orig><reg>volar</reg></choice> <choice><abbr>pēsa</abbr><expan>pensa</expan></choice><choice><orig> &virg;</orig><reg>,</reg></choice> indarno spiega <choice><orig>lale</orig><reg>l’ale</reg></choice>.</l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf183" type="sonnet" n="183"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
