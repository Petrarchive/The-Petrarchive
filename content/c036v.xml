          <lg xml:id="rvf177" type="sonnet" n="177"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#fs24pt #blue">M</hi><hi rendition="#small-caps">i</hi>lle piagge in un giorno<choice><orig> &virg;</orig><reg></reg></choice> &amp7; mille <choice><orig>riui</orig><reg>rivi</reg></choice></l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">Mostrato <choice><orig>ma</orig><reg>m'à</reg></choice> &per; la famosa Ardenna</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
