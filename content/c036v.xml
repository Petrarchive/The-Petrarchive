      <body>
         <pb n="charta 36 verso" facs="../images/vat-lat3195-f/vat-lat3195-f-036v.jpg" /> 
          <lg type="sonnet" xml:id="rvf174" n="174"> 
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#red #fs24pt">F</hi><hi rendition="#small-caps">e</hi>ra stella <choice><orig>sel</orig><reg>se 'l</reg></choice> cielo <choice><orig>s</orig><reg>à</reg></choice> <choice><orig>força</orig><reg>forza</reg></choice> in noi</l> 
                      <l n="2"><choice><orig>Quantalcun</orig><reg>Quanr' alcun</reg></choice> crede&v2c; fu sotto <choice><orig>chio</orig><reg>ch'io</reg></choice> nacqui<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Et fera cuna&v2c; <choice><orig>doue</orig><reg>dove</reg></choice> nato giacqui<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="4">Et fera terra<choice><orig> &virg;</orig><reg></reg></choice> <choice><orig>oue</orig><reg>ove</reg></choice> pie<supplied>'</supplied> mossi poi<choice><orig> .</orig><reg>;</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Et fera <choice><abbr>dōna</abbr><expan>donna</expan></choice>&v2c; che con gli occhi suoi</l>
                      <l n="6">Et con <choice><orig>larco</orig><reg>l'arco</reg></choice><choice><orig> &virg;</orig><reg></reg></choice> a cui sol &per; segno piacqui&p2c;</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Fe<supplied>'</supplied> la piaga&v2c; onde<supplied>,</supplied> <choice><orig>amor</orig><reg>Amor</reg></choice><supplied>,</supplied> teco &non; tacqui&v2c;</l>
                      <l n="8">Che &con; <choice><orig>quellarme</orig><reg>quell'arme</reg></choice> risaldar la poi<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9">Ma tu prendi a diletto i dolor miei<choice><orig> .</orig><reg>;</reg></choice></l>
                      <l n="10">Ella non <choice><orig>gia</orig><reg>già</reg></choice><choice><orig> .</orig><reg>,</reg></choice> <choice><orig>&per;che</orig><reg>perché</reg></choice> non son &piu; duri<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11"><choice><orig>El</orig><reg>E 'l</reg></choice> colpo &e; di saet<hi rendition="#b">ta</hi><choice><orig> .</orig><reg></reg></choice> &amp7; non di spiedo<choice><orig> .</orig><reg>.</reg></choice></l>
                      <l n="12">Pur mi consola<choice><orig> .</orig><reg>,</reg></choice> che languir &per; lei</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Meglio <choice><orig>&#11805;e&#11805;</orig><reg>è</reg></choice> che gioir <choice><orig>daltra</orig><reg>d'altra</reg></choice><choice><orig> .</orig><reg>;</reg></choice> &amp7; tu <choice><orig>mel</orig><reg>me 'l</reg></choice> giuri</l>
                      <l n="14">Per lorato tuo strale<choice><orig> .</orig><reg>,</reg></choice> &amp7; io tel credo.</l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf175" type="sonnet" n="175"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#blue #fs24pt">Q</hi><hi rendition="#small-caps">v</hi>ando mi <choice><orig>uene</orig><reg>vene</reg></choice> <choice><orig>inançi</orig><reg>inanzi</reg></choice> il tempo <choice><orig>el</orig><reg>e 'l</reg></choice> loco</l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2"><choice><orig>Oui</orig><reg>Ov'i'</reg></choice> perdei me stesso<choice><orig> .</orig><reg></reg></choice> <choice><orig>el</orig><reg>e 'l</reg></choice> caro nodo<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3"><choice><orig>Ondamor</orig><reg>Ond' Amor</reg></choice> di sua man <choice><orig>mauinse</orig><reg>m'anvise</reg></choice> &in; modo</l>
                      <l n="4">Che <choice><orig>lamar</orig><reg>l'amar</reg></choice> mi fe<supplied>'</supplied> dolce<choice><orig> .</orig><reg></reg></choice> <choice><orig>el</orig><reg>e 'l</reg></choice> <choice><abbr>piāger</abbr><expan>pianger</expan></choice> gioco<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Solfo &amp7; esca son tutto&p2c; <choice><orig>el</orig><reg>e 'l</reg></choice> cor un foco</l>
                      <l n="6">Da quei <choice><orig>soaui</orig><reg>soavi</reg></choice> spirti&v2c; <choice><orig>iquai</orig><reg>i quai</reg></choice> sempre odo&v2c;</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Acceso dentro &si; <choice><orig>chardendo</orig><reg>ch'ardendo</reg></choice> godo<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="8">Et di <choice><orig>cio</orig><reg>ciò</reg></choice> <choice><orig>uiuo</orig><reg>vivo</reg></choice><choice><orig> .</orig><reg>,</reg></choice> &amp7; <choice><orig>daltro</orig><reg>d'altro</reg></choice> mi cal poco<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9">⸝	Quel sol&v2c; che solo <choice><orig>a gliocchi</orig><reg>agli occhi</reg></choice> mei <choice><abbr>resplēde</abbr><expan>resplende</expan></choice>&v2c;</l>
                      <l n="10">Coi <choice><orig>uaghi</orig><reg>vaghi</reg></choice> raggi anchor indi mi scalda .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">A <choice><orig>uespro</orig><reg>vespro</reg></choice> tal&v2c; qual era oggi &per; tempo<choice><orig> .</orig><reg>;</reg></choice></l>
                      <l n="12">Et <choice><orig>cosi</orig><reg>così</reg></choice> di lontan <choice><orig>malluma</orig><reg>m'alluma</reg></choice> <choice><orig>encende</orig><reg>e 'ncende</reg></choice><choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Che la memoria <choice><orig>adognior</orig><reg>ad ogni or</reg></choice> fresca &amp7; salda<choice><orig> .</orig><reg></reg></choice></l>
                      <l n="14">Pur quel nodo mi mostra <choice><orig>el</orig><reg>e 'l</reg></choice> loco <choice><orig>el</orig><reg>e 'l</reg></choice> <choice><abbr>tēpo</abbr><expan>tempo</expan></choice><choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf176" type="sonnet" n="176"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><choice><orig><hi rendition="#red #fs24pt">P</hi> <hi rendition="#small-caps">e</hi>r|meççi</orig><reg><hi rendition="#red #fs24pt">P</hi> <hi rendition="#small-caps">e</hi>r mezz'i</reg></choice> boschi inhospiti  &amp7; <choice><orig>seluaggi</orig><reg>selvaggi</reg></choice>&v2c;</l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">O<subst><del type="erasure"><gap reason="illegible"/></del><add>nd</add></subst>e <choice><orig><abbr>uāno</abbr><expan>uanno</expan></orig><reg>vanno</reg></choice> a gran rischio <choice><orig>uominj</orig><reg>uomini</reg></choice> &amp7; arme<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Vo securo io&p2c; <choice><orig>che</orig><reg>ché</reg></choice> non <choice><orig>po</orig><reg>pò</reg></choice> <choice><orig>spauerntarme</orig><reg>spaventarme</reg></choice><choice><orig> &virg;</orig><reg></reg></choice></l>
                      <l n="4">Altri <choice><orig>chel</orig><reg>che 'l</reg></choice> sol<choice><orig> &virg-underdot;</orig><reg>,</reg></choice> <choice><orig>cha</orig><reg>ch'à</reg></choice> <choice><orig>damor</orig><reg>d'amor</reg></choice> <choice><orig>uiuo</orig><reg>vivo</reg></choice> i raggi<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Et <choice><orig>uo</orig><reg>vo</reg></choice> cantando&p2c; o pensier miei &non; saggi&p2c;</l>
                      <l n="6">Lei <choice><orig>chel</orig><reg>che 'l</reg></choice> ciel non poria lontana farme<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7"><choice><orig>Chi</orig><reg>Ch'i'</reg></choice> <choice><orig>lo</orig><reg>l'ò</reg></choice> <choice><orig>ne gliocchi</orig><reg>negli occhi</reg></choice><choice><orig> .</orig><reg>,</reg></choice> &amp7; <choice><orig>ueder</orig><reg>veder</reg></choice> seco parme</l>
                      <l n="8"><choice><abbr>Dōne</abbr><expan>Donne</expan></choice><choice><orig> &virg;</orig><reg></reg></choice> et <choice><orig>donçelle</orig><reg>donzelle</reg></choice><choice><orig> .</orig><reg>,</reg></choice> &amp7; sono abeti &amp7; faggi<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><choice><orig>.	Parme</orig><reg>Parme</reg></choice> <choice><orig>dudirla</orig><reg>d'udirla</reg></choice>&p2c; udendo <choice><orig>irami</orig><reg>i rami</reg></choice> <choice><orig>&amp7;lore</orig><reg>et l'ore</reg></choice><choice><orig>.</orig><reg></reg></choice></l>
                      <l n="10">Et le frondi&v2c; &amp7; <choice><orig>gliaugei</orig><reg>gli augei</reg></choice> lagnarsi&v2c; et <choice><orig>lacque</orig><reg>l'acque</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Mormorando fuggir &per; <choice><orig>lerba</orig><reg>l'erba</reg></choice> <choice><orig>uerde</orig><reg>verde</reg></choice>.</l>
                      <l n="12">Raro un silentio&v2c; un solitario horrore</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13"><choice><orig>Dombrosa</orig><reg>D'ombrosa</reg></choice> <choice><orig>selua</orig><reg>selva</reg></choice> mai tanto mi piacque&p2c;</l>
                      <l n="14">Se &non; che dal mio sol troppo si perde.</l>
                  </lg>
