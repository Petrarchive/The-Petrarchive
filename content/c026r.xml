   <text>
      <body>
         <pb n="charta 26 recto" facs="../images/vat-lat3195-f/vat-lat3195-f-026r.jpg"/>
        <ab type="blockSubst" xml:id="rvf121palimpsestGroup">
          <!-- jawalsh: Do we know the sonnet underneath?" -->
          <!-- <note resp="#jaw">Sonnet 194 has been written over an erased version of ???.</note>-->
          <subst>
            <del type="erasure">
              <lg xml:id="rvf121p" type="ballata_mezzana">
                <lg type="dblvrs">
                   <l n="1"><hi rendition="#red #fs24pt">D</hi><choice><abbr>ōna</abbr><expan>onna</expan></choice> 
                    mi <choice><orig>uene</orig><reg>vene</reg></choice> spesso ne la mente<choice><orig> )</orig><reg>;</reg></choice></l>
                   <l n="2">Altra <choice><abbr>dōna</abbr><expan>donna</expan></choice> <choice><orig>ue</orig><reg>v’è</reg></choice> sempre<choice><orig>&#729;/</orig><reg>,</reg></choice></l>
                </lg>
                <lg type="single"><l n="3">Ond<supplied>’ </supplied>io temo si stempre <choice><orig>&virg;</orig><reg/></choice> il cor ardente<choice><orig> .</orig><reg>.</reg></choice></l></lg>
                <lg type="dblvrs">
                  <l n="4"><choice><orig><g ref="#parag"/></orig><reg></reg></choice> Quella il notrica in amorosa &fiamma;<choice><orig> <g ref="#verbar"/></orig><reg/></choice></l>
                  <l n="5">Con un dolce martir pien di desire<choice><orig> .</orig><reg>;</reg></choice></l>
                </lg>
                <lg type="dblvrs">
                  <l n="6">Questa lo strugge oltra mesura <choice><orig>en</orig><reg>e ’n</reg></choice> &fiamma;<choice><orig> <g ref="#verbar"/></orig><reg></reg></choice></l>
                  <l n="7">Tanto <choice><orig>chadoppio</orig><reg>ch’ a doppio</reg></choice> &e; <choice><orig>força</orig><reg>forza</reg></choice> che sospire<choice><orig> .</orig><reg>.</reg></choice></l>
                </lg>
                <lg type="dblvrs">
                   <l n="8"><choice><orig><g ref="#parag"/></orig><reg/></choice> <choice><orig>Ne</orig><reg>Né</reg></choice> <choice><orig>ual</orig><reg>val</reg></choice> <choice><orig>perchio</orig><reg>perch’io</reg></choice> <choice><orig>madire</orig><reg>m’adire</reg></choice><choice><orig> &virg;</orig><reg/></choice> &amp7; arme il core<choice><orig><g ref="#punctelevhiback"/></orig><reg>,</reg></choice></l>
                   <l n="9"><choice><orig>Chi</orig><reg>Ch’i</reg></choice><supplied>’</supplied> non so come <choice><orig>amore</orig><reg>Amore</reg></choice><choice><orig><g ref="#punctelevhiback"/></orig><reg>,</reg></choice></l>
                </lg>
                <l n="10">Di che forte mi sdegno<supplied>,</supplied> lel consente<choice><orig>.,</orig><reg>.</reg></choice></l>    
              </lg>
            </del>
            <add>
         <lg xml:id="rvf121" type="madrigal" n="121">
         <lg type="stanza">
            <lg type="dblvrs" corresp="#canvasline">
               <l n="1"><hi rendition="#red #fs24pt">O</hi>r <choice><orig>uedi</orig><reg>vedi</reg></choice><supplied>,</supplied> <choice><orig>amor</orig><reg>Amor</reg></choice><reg>,</reg> che <choice><orig>giouenetta</orig><reg>giovenetta</reg></choice> 
                  <choice><abbr>dōna</abbr><expan>donna</expan></choice><choice><orig> &virg;</orig><reg/></choice></l>
               <l n="2">Tuo regno <choice><orig>spreçça</orig><reg>sprezza</reg></choice>&p2c; &amp7; del mio mal &non; cura&p2semicolon;</l>
            </lg>
            <l n="3" corresp="#canvasline">&Amp7; tra duo ta<supplied>’</supplied> nemici &e; &si; secura.</l>
            <lg type="dblvrs" corresp="#canvasline">
               <l n="4">Tu <choice><orig>se</orig><reg>sè</reg></choice> armato&p2c; &amp7; ella &in; treccie 
                  <choice><orig><abbr>engōna</abbr><expan>engonna</expan></orig><reg>e ’n gonna</reg></choice><choice><orig> &virg;</orig><reg/></choice></l>
               <l n="5">Si siede<supplied>,</supplied> et <choice><orig>scalça</orig><reg>scalza</reg></choice>&v2c; &in; <choice><orig>meçço</orig><reg>mezzo</reg></choice> i fiori &amp7; <choice><orig>lerba</orig><reg>l’erba</reg></choice>&p2c;</l>
            </lg>
            <l n="6">Ver<supplied>’</supplied> me spietata&v2c; <choice><orig><choice><abbr>encōtra</abbr><expan>encontra</expan></choice></orig><reg>e ’ncontra</reg></choice> 
               te superba.</l>
            <lg type="dblvrs" corresp="#canvasline">
               <l n="7">&Amp7; son pregion<choice><orig> &virg;</orig><reg>;</reg></choice> ma <choice><orig>sepieta</orig><reg>se pietà</reg></choice> 
                  <choice><abbr>āchor</abbr><expan>anchor</expan></choice> serba<choice><orig> &virg;</orig><reg/></choice></l>
               <l n="8"><choice><orig>Larco</orig><reg>L’arco </reg></choice>tuo saldo<choice><orig>.</orig><reg/></choice> &amp7; qualchuna saetta&p2c;</l>
            </lg>
            <l n="9">Fa<supplied>’</supplied> di te<choice><orig> &virg;</orig><reg/></choice> &amp7; di me<supplied>,</supplied> signor<supplied>,</supplied> <choice><orig>uendetta</orig><reg>vendetta</reg></choice>.</l>
         </lg>
         </lg>
         
         <!-- Madrigal 121: Initial (it was D, transformed into O, additional erasure?) 
            + the whole madrigal written over erasure 
            + different handwriting 
         How to encode all this?-->
            </add>
          </subst>
        </ab>
         
            <lg xml:id="rvf122" type="sonnet" n="122">
              <lg type="octave">
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="1"><hi rendition="#blue #fs24pt"
                     >D</hi><hi rendition="#small-caps">i</hi>cesette anni <choice><orig>a</orig><reg>à</reg></choice> <choice><orig>gia</orig><reg>già</reg></choice> <choice><orig>riuolto</orig><reg>rivolto</reg></choice> il cielo</l>
                  <l n="2">Poi <choice><orig>chemprima</orig><reg>che ’mprima</reg></choice> arsi&p2c; &amp7; <choice><orig>gia</orig><reg>già</reg></choice> mai &non; mi 
                     <choice><abbr>spēsi</abbr><expan>spensi</expan></choice>&p2semicolon;</l>
               </lg>
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="3">Ma &quando; <choice><orig>auen</orig><reg>aven</reg></choice> <choice><orig>chal</orig><reg>ch’al</reg></choice> mio stato 
                     <choice><abbr>ripēsi</abbr><expan>ripensi</expan></choice><supplied>,</supplied></l>
                  <l n="4">Sento nel <choice><orig>meçço</orig><reg>mezzo</reg></choice> de le 
                     <choice><abbr>fiāme</abbr><expan>fiamme</expan></choice> un gielo.</l>
               </lg>
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="5">Vero <choice><orig>el</orig><reg>è ’l</reg></choice> <choice><orig>prouerbio</orig><reg>proverbio</reg></choice><choice><orig> &virg;</orig><reg>:</reg></choice> <choice><orig>chaltri</orig><reg>ch’ altri</reg></choice> 
                     <choice><abbr>cāgia</abbr><expan>cangia</expan></choice> il pelo</l>
                  <l n="6"><choice><orig>Ançi</orig><reg>Anzi</reg></choice> <choice><orig>chel</orig><reg>che ’l</reg></choice> <choice><orig>ueçco</orig><reg>vezzo</reg></choice>&p2c; et &per; lentar i sensi</l>
               </lg>
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="7"><choice><orig>Gliumani</orig><reg>Gli umani</reg></choice> affecti &non; son meno 
                     <choice><abbr>intēsi</abbr><expan>intensi</expan></choice>&p2semicolon;</l>
                  <l n="8"><choice><orig>Cio</orig><reg>Ciò</reg></choice> ne fa <choice><orig>lombra</orig><reg>l’ombra</reg></choice> ria del <choice><orig>graue</orig><reg>grave</reg></choice> <choice><orig>uelo</orig><reg>velo</reg></choice>.</l>
               </lg>
              </lg>
              <lg type="sestet">
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="9"><choice><orig>Oi me</orig><reg>Oimè</reg></choice> lasso<choice><orig> &virg;</orig><reg>!</reg></choice> e &quando; fia quel giorno</l>
                  <l n="10">Che mirando il fuggir <choice><orig>de <abbr>gliāni</abbr><expan>glianni</expan></orig><reg>degli anni</reg></choice> miei</l>
               </lg>
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="11">Esca del foco<choice><orig>.</orig><reg/></choice> et di &si; lunghe pene<choice><orig>.</orig><reg>?</reg></choice></l>
                  <l n="12"><choice><orig>Vedro</orig><reg>Vedrò</reg></choice> mai il <choice><orig>di</orig><reg>dì</reg></choice> che pur <choice><orig>quantio</orig><reg>quant’io</reg></choice> <choice><orig>uorrei</orig><reg>vorrei</reg></choice></l>
               </lg>
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="13">Quel<supplied>’</supplied> aria dolce del bel <choice><orig>uiso</orig><reg>viso</reg></choice> adorno</l>
                  <l n="14">Piaccia a <choice><orig>questocchi</orig><reg>quest’occhi</reg></choice>&p2c; &amp7; quanto si <choice><orig><abbr>cōuene</abbr><expan>conuene</expan></orig><reg>convene</reg></choice><g ref="#punctinter"/></l>
               </lg>
              </lg>
            </lg>
         
         <!-- Sonnet 122: End of line 14: punctus elevatus? -->
         
            <lg xml:id="rvf123" type="sonnet" n="123">
              <lg type="octave">
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="1"><hi rendition="#red #fs24pt"
                     >Q</hi><hi rendition="#small-caps"><choice><orig>v</orig><reg>u</reg></choice></hi>el <choice><orig>uago</orig><reg>vago</reg></choice> impallidir che<reg> ’</reg>l dolce riso</l>
                  <l n="2">D<reg>’</reg>un<reg>’</reg>amorosa nebbia ricoperse<reg>,</reg></l>
               </lg>
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="3">Con tanta maiestade al cor <choice><orig>sofferse</orig><reg>s’offerse</reg></choice><supplied>,</supplied></l>
                  <l n="4">Che li si fece <choice><orig>incontra</orig><reg>incontr’ a</reg></choice> <choice><orig>meççol</orig><reg>mezzo ’l</reg></choice> <choice><orig>uiso</orig><reg>viso</reg></choice>.</l>
               </lg>
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="5">Conobbi allor &si; come in paradiso</l>
                  <l n="6">Vede <choice><orig>lun</orig><reg>l’un</reg></choice> <choice><orig>laltro</orig><reg>l’altro</reg></choice>&p2c; in tal guisa <choice><orig>saperse</orig><reg>s’aperse</reg></choice></l>
               </lg>
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="7">Quel pietoso penser <choice><orig>chaltri</orig><reg>ch’altri</reg></choice> &non; scerse&p2semicolon;</l>
                  <l n="8">Ma <choice><orig>uidilio</orig><reg>vidil’io</reg></choice>&v2c; <choice><orig>chaltroue</orig><reg>ch’altrove</reg></choice> &non; <choice><orig>maffiso</orig><reg>m’affiso</reg></choice><supplied>.</supplied></l>
               </lg>
              </lg>
              <lg type="sestet">
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="9">Ogni angelica <choice><orig>uista</orig><reg>vista</reg></choice>&p2c; <choice><orig>ogniatto</orig><reg>ogni atto</reg></choice> humile</l>
                  <l n="10">Che giamai &in; <choice><abbr>dōna</abbr><expan>donna</expan></choice>
                     <choice><orig>ouamor</orig><reg>ov’amor</reg></choice> fosse <choice><orig>apparue</orig><reg>apparve</reg></choice><supplied>,</supplied></l>
               </lg>
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="11">Fora uno sdegno a lato a quel <choice><orig>chi</orig><reg>ch’i</reg></choice><supplied>’</supplied> dico.</l>
                  <l n="12"><choice><orig>Chinaua</orig><reg>Chinava</reg></choice> a terra il bel guardo gentile&p2c;</l>
               </lg>
               <lg type="dblvrs" corresp="#canvasline">
                  <l n="13">Et tacendo dicea&v2c; come a me <choice><orig>parue</orig><reg>parve</reg></choice><choice><orig> &virg;</orig><reg>:</reg></choice></l>
                  <l n="14">Chi <choice><orig>mallontana</orig><reg>m’allontana</reg></choice> il mio fedele amico<choice><orig>.</orig><reg>?</reg></choice></l>
               </lg>
              </lg>
            </lg>
         <lg xml:id="rvf124" type="sonnet" n="124">
           <lg type="octave">
            <lg type="dblvrs" corresp="#canvasline">
               <l n="1"><hi rendition="#blue #fs24pt"
                  >A</hi><hi rendition="#small-caps">m</hi>or<supplied>,</supplied> fortuna&v2c; 
                  et la mia mente<supplied>,</supplied> <choice><orig>schiua</orig><reg>schiva</reg></choice></l>
               <l n="2">Di quel che <choice><orig>uede</orig><reg>vede</reg></choice><choice><orig> &v2c;</orig><reg/></choice> e nel passato <choice><orig>uolta</orig><reg>volta</reg></choice><supplied>,</supplied></l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
               <l n="3"><choice><orig>Maffligon</orig><reg>M’affligon</reg></choice> &si; <choice><orig>chio</orig><reg>ch’io</reg></choice> porto alcuna <choice><orig>uolta</orig><reg>volta</reg></choice></l>
               <l n="4"><choice><orig>Inuadia</orig><reg>Invidia</reg></choice> a quei che son su <choice><orig>laltra</orig><reg>l’altra</reg></choice> <choice><orig>riua</orig><reg>riva</reg></choice>.</l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
               <l n="5">Amor mi <choice><orig>struggel</orig><reg>strugge ’l</reg></choice> cor&v2c; fortuna il <choice><orig>priua</orig><reg>priva</reg></choice></l>
               <l n="6"><choice><orig>Dogni</orig><reg>D’ogni</reg></choice> conforto&p2c; onde la mente stolta<choice><orig>.</orig><reg/></choice></l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
               <l n="7"><choice><orig>Sadira</orig><reg>S’adira</reg></choice> &amp7; piange&p2semicolon; &amp7; <choice><orig>cosi</orig><reg>così</reg></choice> in pena molta</l>
               <l n="8">Sempre <choice><orig>conuen</orig><reg>conven</reg></choice> che 
                  <choice><abbr>cōbattendo</abbr><expan>combattendo</expan></choice> <choice><orig>uiua</orig><reg>viva</reg></choice>.</l>
            </lg>
           </lg>
           <lg type="sestet">
            <lg type="dblvrs" corresp="#canvasline">
               <l n="9"><choice><orig>Ne</orig><reg>Né</reg></choice> spero i dolci <choice><orig>di</orig><reg>dì</reg></choice> tornino <choice><orig>in dietro</orig><reg>indietro</reg></choice><supplied>,</supplied></l>
               <l n="10">Ma pur di male &in; peggio quel <choice><orig>chauança</orig><reg>ch’avanza</reg></choice><supplied>;</supplied></l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
               <l n="11">Et di mio corso <choice><orig>o</orig><reg>ò</reg></choice> <choice><orig>gia</orig><reg>già</reg></choice> <choice><orig>passatol</orig><reg>passato ’l</reg></choice> <choice><orig>meçço</orig><reg>mezzo</reg></choice>.</l>
               <l n="12">Lasso<supplied>!</supplied> &non; di diamante&v2c; ma <choice><orig>dun</orig><reg>d’un</reg></choice> <choice><orig>uetro</orig><reg>vetro</reg></choice></l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
               <l n="13">Veggio di man cadermi ogni <choice><orig>sperança</orig><reg>speranza</reg></choice>&p2c;</l>
               <l n="14">Et tutti miei pensier<supplied>’</supplied> romper nel <choice><orig>meçço</orig><reg>mezzo</reg></choice>.</l>
            </lg>
           </lg>
