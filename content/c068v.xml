   <text>
      <body>
          
          <pb xml:id="c068v" n="charta 68 verso" facs="../images/vat-lat3195-f/vat-lat3195-f-068v.jpg"/>
          <lg type="sonnet" xml:id="rvf351" n="349 [351]">
              <note type="sequence">
                  <bibl>
                      <idno type="rev1">351</idno>
                      <idno type="physical_vl3195">349</idno>
                  </bibl>
              </note> <!-- check note --> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#fs24pt">E</hi> mi par dor &in; hora udire il messo</l> 
                      <l n="2">Che <choice><abbr>madōna</abbr><expan>madonna</expan></choice> mi <choice><abbr>māde</abbr><expan>mande</expan></choice> ase chiamando .</l><fw>.14.</fw>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Cosi <choice><abbr>dētro</abbr><expan>dentro</expan></choice> &amp7;di for mi uo <choice><abbr>cāgiando</abbr><expan>cangiando</expan></choice> .</l>
                      <l n="4">&Amp7; sono &in; &non; <choice><abbr>moltāni</abbr><expan>moltanni</expan></choice> si dimesso .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Cha pena riconosco omai me stesso .</l>
                      <l n="6">Tuttol uiuer usato o messo &in; bando .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Sarei <choice><abbr>cōtento</abbr><expan>contento</expan></choice> di sapere il &quando;.</l>
                      <l n="8">Ma pur deurebbe il <choice><abbr>tēpo</abbr><expan>tempo</expan></choice> esser da presso .</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><hi rendition="#sestet_maker">·</hi>O felice quel di &virg; che del terreno</l><!-- check all sestet markers -->
                      <l n="10">Carcere uscendo lasci rotta &amp7; sparta</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Questa mia graue &amp7; frale &amp7; mortal <choice><abbr>gōna</abbr><expan>gonna</expan></choice>.</l>
                      <l n="12">&Amp7; dasi folte tenebre mi parta</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13"><choice><abbr>Volādo</abbr><expan>Volando</expan></choice> <choice><abbr>tāto</abbr><expan>tanto</expan></choice> su nelbel sereno &virg;</l>
                      <l n="14">Chi ueggia&virg;ilmio signore &virg;&amp7; lamia <choice><abbr>dōna</abbr><expan>donna</expan></choice> .</l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf356" type="sonnet" n="352 [356]">
              <note type="sequence">
                  <bibl>
                      <idno type="rev1">356</idno>
                      <idno type="physical_vl3195">352</idno>
                  </bibl>
              </note><!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#fs24pt">L</hi>aura mia sacra almio <choice><abbr>stāco</abbr><expan>stanco</expan></choice> riposo .</l>
                      <l n="2">Spira sispesso chi prendo ardimento</l><fw>21</fw><!-- 21 is inscribed in a circle -->
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Di dirle ilmal chi <choice><abbr>osētit</abbr><expan>osentit</expan></choice> &amp7; sento .</l>
                      <l n="4">Che <choice><abbr>uiuēdo</abbr><expan>uiuendo</expan></choice> ella&virg;&non; sarei stat&#7885; oso .</l><!-- check underdotted o (see also line 5)--> 
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">I&#7885; <choice><abbr>īcomīcio</abbr><expan>incomincio</expan></choice> daquel guardo amoroso .</l>
                      <l n="6">Che fu <choice><abbr>prīcipio</abbr><expan>principio</expan></choice> asi <choice><abbr>lūgo</abbr><expan>lungo</expan></choice> <choice><abbr>tormēto</abbr><expan>tormento</expan></choice> .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Poi seguo come misero &virg;&amp7; <choice><abbr>cōtento</abbr><expan>contento</expan></choice> &virg;</l>
                      <l n="8">Di di in di &virg; dora &in; hora &virg; amor ma roso .</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><hi rendition="#sestet_maker">·</hi>Ella si tace &amp7; di pieta depinta</l>
                      <l n="10">Fiso mira pur me . parte sospira .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">&Amp7; di lagrime honeste il uiso adorna</l>
                      <l n="12">Onde lanima mia dal dolor uinta</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13"><choice><abbr>Mētre</abbr><expan>Mentre</expan></choice> <choice><abbr>piāgēdo</abbr><expan>piangendo</expan></choice> allor <hi rendition="#b">seco sa</hi>dira</l>
                      <l n="14">Sciolta dal <choice><abbr>sōno</abbr><expan>sonno</expan></choice> a sestessa ritorna .</l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf357" type="sonnet" n="353 [357]">
              <note type="sequence">
                  <bibl>
                      <idno type="rev1">357</idno>
                      <idno type="physical_vl3195">353</idno>
                  </bibl>
              </note><!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#fs24pt">O</hi>gni giorno mipar piu <choice><abbr>dimillāni</abbr><expan>dimillanni</expan></choice></l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">Chi segua lamia fida &amp7; cara duce .</l><fw>22.</fw>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Che <choice><abbr>micōdusse</abbr><expan>micondusse</expan></choice> <choice><abbr>almōdo</abbr><expan>almondo</expan></choice>&#x007c;or <choice><abbr>micōduce</abbr><expan>miconduce</expan></choice></l><!-- check vertical bar -->
                      <l n="4">&Per; miglior uia&virg;auita sen&#xa763;a <choice><abbr>affāni</abbr><expan>affanni</expan></choice> .</l><!-- check ç also in last sonnet-->
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">&Amp7; &non; mi posson ritener linganni</l>
                      <l n="6"><choice><abbr>Delmōdo</abbr><expan>Delmondo</expan></choice> . chil conosco .&amp7; <choice><abbr>tāta</abbr><expan>tanta</expan></choice> luce</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7"><choice><abbr>Dētro</abbr><expan>Dentro</expan></choice> almio core&virg;&in;fin dal ciel traluce</l><!-- check abbreviation in: separarte words? -->
                      <l n="8"><choice><abbr>Chincomīcio</abbr><expan>Chincomincio</expan></choice> <choice><abbr>acōtar</abbr><expan>acontar</expan></choice> iltempo &virg;<choice><abbr>eidāni</abbr><expan>eidanni</expan></choice> .</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><hi rendition="#sestet_maker">~</hi>Ne minaccie temer debbo dimorte</l><!-- check sestet marker -->
                      <l n="10">Chel re sofferse &con; piu graue pena .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">&Per; farme a seguitar <choice><abbr>cōstāte</abbr><expan>constante</expan></choice> &amp7; forte .</l>
                      <l n="12">&Amp7; or <choice><abbr>nouellamēte</abbr><expan>nouellamente</expan></choice> &in; ogni uena</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Intro dilei che mera data in sorte .</l>
                      <l n="14">&Amp7; &non; turbo lasua fronte serena .</l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf358" type="sonnet" n="354 [358]">
              <note type="sequence">
                  <bibl>
                      <idno type="rev1">358</idno>
                      <idno type="physical_vl3195">354</idno>
                  </bibl>
              </note><!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#fs24pt">N</hi>on po far morte ildolce uiso amaro .</l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">Maldolce uiso dolce po far morte .</l><fw>23</fw>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Che bisogn&#7841; amorir ben&#x007c;altre scorte .</l><!-- check underdotted a: use tag to delete? check vertical bar -->
                      <l n="4">Quella miscorge ondogni ben <choice><abbr>īparo</abbr><expan>inparo</expan></choice> .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Etquei che delsuo <choice><abbr>sāgue</abbr><expan>sangue</expan></choice> &non; fu auaro .</l>
                      <l n="6">Che col pe ruppe le tartaree porte .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Col suo morir par che mi <choice><abbr>ricōforte</abbr><expan>riconforte</expan></choice>.</l>
                      <l n="8"><choice><abbr>Dūque</abbr><expan>Dunque</expan></choice> uien morte . iltuo uenir me caro .</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><hi rendition="#sestet_maker">/~</hi>&Amp7; &non; tardar &v2c;cheglie ben tempo omai</l><!-- check the two sestet markers -->
                      <l n="10">Et se &non; fusse &virg;e ful <choice><abbr>tēpo</abbr><expan>tempo</expan></choice> &in; quel <choice><abbr>pūto</abbr><expan>punto</expan></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Che <choice><abbr>madōna</abbr><expan>madonna</expan></choice> passo di questa uita .</l>
                      <l n="12">Dallor <choice><abbr>īnā&#xa763;i</abbr><expan>innan&#xa763;i</expan></choice> un di &non; uissi mai .</l><!-- check ç  -->
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Seco fui &in; uia . &amp7; seco alfin <choice><abbr>songiūto</abbr><expan>songiunto</expan></choice> .</l>
                      <l n="14">&Amp7; mia giornata o cosuoi pie fornita .</l>
                  </lg>
              </lg>
          </lg>
      </body>
   </text>
