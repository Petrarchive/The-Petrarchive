    <text>
        <body>
            <pb n="charta 62 recto" facs="../images/vat-lat3195-f/vat-lat3195-f-062r.jpg" /> <!-- Change the number of the charta in both places "charta 1 verso" and -f-0001v.jpg", according to the charta to be encoded -->
            <lg type="sonnet" xml:id="rvf317" n="317"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
                <lg type="octave">
                    <lg type="dblvrs" corresp="#canvasline">
                        <l n="1"><hi rendition="#red #fs24pt">T</hi><hi rendition="#small-caps">r</hi>anquillo porto <choice><orig>auea</orig><reg>avea</reg></choice> mostrato amore</l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                        <l n="2">A la mia lunga &amp7; torbida tempesta</l>
                    </lg>
                    <lg type="dblvrs" corresp="#canvasline">
                        <l n="3">Fra <choice><orig><abbr>gliāni</abbr><expan>glianni</expan></orig><reg>gli anni</reg></choice> de la <choice><orig>eta</orig><reg>età</reg></choice> matura honesta</l>
                        <l n="4">Che i <choice><orig>uicij</orig><reg>vicii</reg></choice> spoglia<choice><orig> .</orig><reg>, </reg></choice>&amp7; <choice><orig>uertu</orig><reg>vertù</reg></choice> <choice><orig>ueste</orig><reg>veste</reg></choice> e <choice><orig><abbr>hono&#x0303;e</abbr><expan>honore</expan></orig><reg>honore</reg></choice><choice><orig> .</orig><reg>.</reg></choice></l>
                    </lg>
                    <lg type="dblvrs" corresp="#canvasline">
                        <l n="5"><choice><orig>Gia</orig><reg>Già</reg></choice> <choice><orig>traluceua</orig><reg>traluceva</reg></choice><choice><orig>  </orig><reg> </reg></choice><choice><orig>a</orig><reg>a’</reg></choice> <choice><orig>begliocchi</orig><reg>begli occhi</reg></choice> il mio core<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                        <l n="6">Et <choice><orig>lalta</orig><reg>l’alta</reg></choice> fede non <choice><orig>piu</orig><reg>più</reg></choice> lor molesta<choice><orig> .</orig><reg>.</reg></choice></l>
                    </lg>
                    <lg type="dblvrs" corresp="#canvasline">
                        <l n="7">Ai<supplied>!,</supplied> morte ria<choice><orig> .</orig><reg>,</reg></choice> come  a schiantar <choice><orig>se</orig><reg>sè</reg></choice> presta</l>
                        <l n="8">Il frutto de <choice><orig><abbr>moltāni</abbr><expan>moltanni</expan></orig><reg>molt’anni</reg></choice> in <choice><orig>si</orig><reg>sì</reg></choice> poche hore<choice><orig> .</orig><reg>.</reg></choice></l>
                    </lg>
                </lg>
                <lg type="sestet">
                    <lg type="dblvrs" corresp="#canvasline">
                        <l n="9"><hi rendition="#sestet_marker">~</hi>Pur <choice><orig>uiuendo</orig><reg>vivendo</reg></choice> <choice><orig>ueniasi</orig><reg>veniasi</reg></choice><supplied>,</supplied> <choice><orig>oue</orig><reg>ove</reg></choice> deposto</l>
                        <l n="10">In quelle caste orecchie aurei <choice><abbr>parlādo</abbr><expan>parlando</expan></choice></l>
                    </lg>
                    <lg type="dblvrs" corresp="#canvasline">
                        <l n="11"><choice><orig>De</orig><reg>De’</reg></choice> miei dolci pensier <choice><orig>lantiqua</orig><reg>l’antiqua</reg></choice> soma<choice><orig>;</orig><reg>.</reg></choice></l>
                        <l n="12">Et ella <choice><orig>aurebbe</orig><reg>avrebbe</reg></choice> a me forse resposto</l>
                    </lg>
                    <lg type="dblvrs" corresp="#canvasline">
                        <l n="13">Qualche santa parola sospirando<choice><orig>.</orig><reg>,</reg></choice></l>
                        <l n="14">Cangiati i <choice><orig>uolti</orig><reg>volti</reg></choice><choice><orig>.</orig><reg>,</reg></choice> &amp7; <choice><orig>luna</orig><reg>l’una</reg></choice> &amp7; <choice><orig>laltra</orig><reg>l’altra</reg></choice> coma.</l>
                    </lg>
                </lg>
            </lg>
            <space corresp="#canvasline"/>
            <lg xml:id="rvf318" type="sonnet" n="318"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
                <lg type="octave">
                    <lg type="dblvrs" corresp="#canvasline">
                        <l n="1"><hi rendition="#blue #fs24pt">A</hi><hi rendition="#small-caps">l</hi> cader <choice><orig>duna</orig><reg>d’una</reg></choice> pianta  che si <choice><orig>suelse</orig><reg>svelse</reg></choice></l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                        <l n="2">Come quella che ferro <choice><orig>&virg; </orig><reg></reg></choice>o <choice><orig>uento</orig><reg>vento</reg></choice> sterpe<choice><orig> .</orig><reg>,</reg></choice></l>
                    </lg>
