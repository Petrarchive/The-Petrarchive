   <text>
      <body>
          
         
          <pb xml:id="c068r" n="charta 68 recto" facs="../images/vat-lat3195-f/vat-lat3195-f-068r.jpg" /> <!-- Change the number of the charta in both places "charta 1 verso" and -f-0001v.jpg", according to the charta to be encoded -->
          <lg type="sonnet" xml:id="rvf345" n="347 [345]"><!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <note type="sequence">
                  <bibl>
                      <idno type="rev1">345</idno>
                      <idno type="physical_vl3195">347</idno>
                  </bibl>
              </note>
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#fs24pt">S</hi><hi rendition="#small-caps"><!-- second letter --></hi>pinse amor &amp7; dolor oue ir &non; debbe<!-- rest of the line --></l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">Lamia <choice><abbr>līgua</abbr><expan>lingua</expan></choice> auiata <choice><abbr>alamētarsi</abbr><expan>alamentarsi</expan></choice><fw><subst><del type="erasure"><gap reason="illegible"/></del><add>10.</add></subst></fw></l><!-- check fw -->
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">A dir di lei &per;chio cantai &amp7; arsi</l>
                      <l n="4">Quel che se fusse uer&virg;torto sarebbe .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Chassailmio stato rio &q-macron;tar deurebbe</l><!-- abbreviation? -->
                      <l n="6">Quella beata . elcor <choice><abbr>raccōsolarsi</abbr><expan>racconsolarsi</expan></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Vedendo <choice><abbr>tāto</abbr><expan>tanto</expan></choice> lei domesticarsi</l>
                      <l n="8">&Con; colui che uiuendo &in; cor &sempre;sēpre ebbe</l><!-- check entity for Cō; also line 13 -->
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><hi rendition="#sestet_maker">·</hi>&Amp7; ben macqueto .&amp7; me stesso <choice><abbr>cōsolo</abbr><expan>consolo</expan></choice> .</l><!-- space after sestet marker? -->
                      <l n="10">Ne uorrei riuederla &in; <choice><abbr>&q-macron;sto</abbr><expan>questo</expan></choice> inferno .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Ançi uoglio morire &virg;&amp7; uiuer solo .</l>
                      <l n="12">Che piu bella che mai &con; locchio <choice><abbr>īterno</abbr><expan>interno</expan></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">&Con; <choice><abbr>liāgeli</abbr><expan>liangeli</expan></choice> la ueggio alçata a uolo</l>
                      <l n="14">A pie delsuo &amp7; mio signore eterno .</l>
                  </lg>
              </lg>
          </lg>
          
          <space corresp="#canvasline"/>
          <lg xml:id="rvf346" type="sonnet" n="348 [346]"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <note type="sequence">
                  <bibl>
                      <idno type="rev1">346</idno>
                      <idno type="physical_vl3195">348</idno>
                  </bibl>
              </note>
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#fs24pt">L</hi>iangeli electi &virg;&amp7; lanime beate</l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">Cittadine del cielo il primo giorno &amp7;<fw>.<subst><del type="erasure"><gap reason="illegible"/></del><add>11.</add></subst></fw></l><!-- check fw -->
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Che <choice><abbr>madōna</abbr><expan>madonna</expan></choice> passo le f&#803;ur intorno .</l>
                      <l n="4">Piene di merauiglia &amp7; di pietate .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Cheluce e questa &virg;&amp7; qual noua beltate <g ref="#punctinter"/>;</l>
                      <l n="6">Dicean tralor . &per; chabito si adorno &virg;</l><!-- punto -->
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Dal <choice><abbr>mōdo</abbr><expan>mondo</expan></choice> <choice><abbr>errāte</abbr><expan>errante</expan></choice> <choice><abbr>a&q-macron;stalto</abbr><expan>aquestalto</expan></choice> soggiorno</l>
                      <l n="8">Non sali mai &in; tutta questa etate<g ref="#punctinter"/>;</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><hi rendition="#sestet_maker">·</hi>Ella <choice><abbr>cōtenta</abbr><expan>contenta</expan></choice> auer <choice><abbr>cāgiato</abbr><expan>cangiato</expan></choice> albergo</l>
                      <l n="10">Siparagona pur coi piu &per;fecti .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">&Amp7; parte ador ador si uolge a tergo .</l>
                      <l n="12"><choice><abbr>Mirādo</abbr><expan>Mirando</expan></choice> sio la seguo . &amp7; par chaspecti .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Ondio uoglie &amp7; <choice><abbr>pēsier</abbr><expan>pensier</expan></choice> tutti alciel ergo</l>
                      <l n="14">&Per; chi lodo pregar pur chi maffretti.</l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          
          <lg xml:id="rvf347" type="sonnet" n="349 [347]"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <note type="sonnet">
              <bibl>
                  <idno type="rev1">347</idno>
                  <idno type="physical_vl3195">349</idno>
              </bibl>
          	  </note>
			  <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#fs24pt">D</hi><hi rendition="#small-caps"><choice><abbr>ōna</abbr><expan>onna</expan></choice> che lieta <choice><abbr>colprīcipio</abbr><expan>colprincipio</expan></choice> nostro</hi> </l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">Tistai &virg;come tua uita <subst><del type="erasure"><gap reason="illegible"/></del><add>alm</add></subst>a rechiede .<fw><subst><del type="erasure"><gap reason="illegible"/></del><add>12.</add></subst></fw></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Assisa &in; alta &amp7; gloriosa sede .</l>
                      <l n="4">&Amp7; daltro ornata che di perle &virg; o dostro .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">O dele <choice><abbr>dōne</abbr><expan>donne</expan></choice> altero &amp7; ra<subst><del type="erasure"><gap reason="illegible"/></del><add>r</add></subst>o mostro .</l>
                      <l n="6">Or neluolto dilui che tutto uede</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Vedil mio amore . &amp7; quella pura fede</l>
                      <l n="8">&Per; chio <choice><abbr>tāte</abbr><expan>tante</expan></choice> uersai&#11805;lagrime &#11805; enchiostro .</l><!-- check punctuation: left lower slanted stroke? -->
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9">&Amp7; <choice><abbr>sēti</abbr><expan>senti</expan></choice> &virg; che uer te i&#803;lmio core in terra</l><!-- check 'i' with dot below -->
                      <l n="10">Tal fu &virg; qual ora e &in; cielo . &amp7; mai &non; uolsi &virg;</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Altro da te chel sol deliocchi tuoi</l>
                      <l n="12"><choice><abbr>Dūque</abbr><expan>Dunque</expan></choice> &per; <choice><abbr>amendar</abbr><expan>amendar</expan></choice> <choice><abbr>lalūga</abbr><expan>lalunga</expan></choice> guerra</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">&Per; cui <choice><abbr>dalmōdo</abbr><expan>dalmondo</expan></choice> ate sola miuolsi &virg;</l>
                      <l n="14">Prega chi <choice><abbr>uenga</abbr><expan>uenga</expan></choice> tosto astar &con; uoi.</l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          
          <lg xml:id="rvf348" type="sonnet" n="350 [348]"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <note type="sonnet">
              <bibl>
                  <idno type="rev1">348</idno>
                  <idno type="physical_vl3195">350</idno>
              </bibl>
          </note><!-- here? -->
			  <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#fs24pt">D</hi><hi rendition="#small-caps">apiu belliocchi &#11789;&amp7; dal piuchiaro uiso</hi></l> <!-- CHECK PUNCTUATION --><!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">Che mai <choice><abbr>splēdesse</abbr><expan>splendesse</expan></choice> &virg;&amp7; dapiu bei capelli &virg;<fw><subst><del type="erasure"><gap reason="illegible"/></del><add>13</add></subst></fw></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Che facean loro elsol parer menbelli</l>
                      <l n="4">Dal piu dolce parlare &amp7; dolce riso &virg;</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Daleman dalebraccia &virg; che conquiso</l>
                      <l n="6">Sença mouersi aurian quai piu rebelli</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Fur damor mai &virg; dapiu bei piedi snelli</l>
                      <l n="8">Dala &per;sona fatta &in; paradiso .</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><hi rendition="#sestet_maker">·</hi><choice><abbr>Prēdean</abbr><expan>Prendean</expan></choice> uita imiei spirti . or na diletto .</l>
                      <l n="10">Il re celeste . I suoi alati corrieri .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">&Amp7; io son qui rimaso ignudo &amp7; cieco .</l>
                      <l n="12">Sol un <choice><abbr>cōforto</abbr><expan>conforto</expan></choice> ale mie pene aspetto .</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Chella che uede tutti miei penseri</l>
                      <l n="14">Mimpetre gratia&virg;chi possa esser seco .</l>
                  </lg>
              </lg>
          </lg>
      </body>
   </text>
