      <body>
         <pb n="charta 57 recto" facs="../images/vat-lat3195-f/vat-lat3195-f-057r.jpg" /> <!-- Change the number of the charta in both places "charta 1 verso" and -f-0001v.jpg", according to the charta to be encoded -->
          <lg type="sonnet" xml:id="rvf277" n="277"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><choice><orig><hi rendition="#red #fs24pt">S</hi><hi rendition="#small-caps">a</hi>mor</orig><reg><hi rendition="#red #fs24pt">S</hi><hi rendition="#small-caps">’a</hi>mor</reg></choice> <choice><orig>nouo</orig><reg>novo</reg></choice> consiglio &non; <choice><orig>napporta</orig><reg>n’apporta</reg></choice><supplied>,</supplied></l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">&Per; <choice><orig>força</orig><reg>forza</reg></choice> <choice><orig>conuerra</orig><reg>converrà</reg></choice> <choice><orig>chel</orig><reg>che ’l</reg></choice> <choice><orig>uiuer</orig><reg>viver</reg></choice> cange<choice><orig> .</orig><reg>;</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Tanta paura &amp7; duol <choice><orig>lalma</orig><reg>l’alma</reg></choice> trista ange<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="4"><choice><orig>Chel</orig><reg>Che ’l</reg></choice> desir <choice><orig>uiue</orig><reg>vive</reg></choice><choice><orig> .</orig><reg>,</reg></choice> &amp7; la <choice><orig>sperança</orig><reg>speranza</reg></choice> <choice><orig>e</orig><reg>è</reg></choice> mort<subst><del type="erasure"><gap reason="illegible"/><add>a</add></del></subst><choice><orig> .</orig><reg>;</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Onde si <subst><del type="erasure"><gap reason="illegible"/><add>sbi</add></del></subst>gottisce<choice><orig> </orig><reg></reg></choice> et si sconforta</l>
                      <l n="6">Mia <choice><orig>uita</orig><reg>vita</reg></choice> in tutto<choice><orig> .</orig><reg>,</reg></choice> &amp7; notte &amp7; giorno piange<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Stanca <choice><orig>sença</orig><reg>senza</reg></choice> <choice><orig>gouerno</orig><reg>governo</reg></choice> &in; mar che <choice><abbr>frāge</abbr><expan>frange</expan></choice><choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="8"><choice><orig>En</orig><reg>E ’n</reg></choice> dubbia <choice><orig>uia</orig><reg>via</reg></choice> <choice><orig>sença</orig><reg>senza</reg></choice> fidata scorta<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><choice><orig><hi rendition="#sestet_maker">~</hi> </orig><reg></reg></choice>Imaginata guida la conduce<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="10"><choice><orig>Che</orig><reg>Ché</reg></choice> la <choice><orig>uera</orig><reg>vera</reg></choice><choice><orig>  </orig><reg> </reg></choice><choice><orig>e</orig><reg>è</reg></choice> sotterra<choice><orig> .</orig><reg>,</reg></choice> <choice><orig>ançi</orig><reg>anzi</reg></choice> <choice><orig>e</orig><reg>è</reg></choice> nel cielo<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Onde <choice><orig>piu</orig><reg>più</reg></choice> che mai chiara al cor traluce<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="12"><choice><orig>A gliocchi</orig><reg>Agli occhi</reg></choice> no<choice><orig> &virg-underdot;</orig><reg>;</reg></choice> <choice><orig>chun</orig><reg>ch’ un</reg></choice> doloroso <choice><orig>uelo</orig><reg>velo</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Contende lor da di<subst><del type="erasure"><gap reason="illegible"/><add>siata</add></del></subst> luce<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="14">Et me fa <choice><orig>si</orig><reg>sì</reg></choice> <choice><orig>pertempo</orig><reg>per tempo</reg></choice> cangiar pelo<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
          </lg>
