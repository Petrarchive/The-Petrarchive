   <text>
      <body>
         <pb n="charta 36 recto" facs="../images/vat-lat3195-f/vat-lat3195-f-036r.jpg" /> 
          <lg type="sonnet" xml:id="rvf170" n="170"> 
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><choice><orig><hi rendition="#red #fs24pt">P</hi><hi rendition="#small-caps">i</hi>u</orig><reg><hi rendition="#red #fs24pt">P</hi><hi rendition="#small-caps">i</hi>ù</reg></choice> <choice><orig>uolte</orig><reg>volte</reg></choice> <choice><orig>gia</orig><reg>già</reg></choice> dal bel <choice><abbr>sembiāte</abbr><expan>sembiante</expan></choice> humano</l> 
                      <l n="2"><choice><orig>O</orig><reg>Ò</reg></choice> preso ardir co le mie fide scorte</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3"><choice><orig>Dassalir</orig><reg>D'assalir</reg></choice> &con; parole honeste accorte</l>
                      <l n="4">La mia nemica in atto humile &amp7; piano<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5"><choice><abbr>Fāno</abbr><expan>Fanno</expan></choice> poi <choice><orig>gliocchi</orig><reg>gli occhi</reg></choice> suoi mio <choice><abbr>pēser</abbr><expan>penser</expan></choice> <choice><orig>uano</orig><reg>vano</reg></choice></l>
                      <l n="6"><choice><orig>&Per; chogni</orig><reg>Perch'ogni</reg></choice> mia fortuna&v2c; ogni mia sorte<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Mio ben&v2c; mio male<choice><orig> &virg;</orig><reg></reg></choice> &amp7; mia <choice><orig>uita</orig><reg>vita</reg></choice><choice><orig> &virg;</orig><reg> </reg></choice>&amp7; mia morte<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="8">Quei che solo il <choice><orig>po</orig><reg>pò</reg></choice> far&v2c; <choice><orig>la</orig><reg>l'à</reg></choice> posto <choice><orig><abbr>īmano</abbr><expan>inmano</expan></orig><reg>in mano</reg></choice><choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><choice><orig>⸝	Ondio</orig><reg>Ond'io</reg></choice> non <choice><orig>pote</orig><reg>poté'</reg></choice> mai formar parola</l>
                      <l n="10"><choice><orig>Chaltro</orig><reg>Ch'altro</reg></choice> che da me stesso fosse intesa<choice><orig> .</orig><reg>;</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11"><choice><orig>Cosi</orig><reg>Così</reg></choice> <choice><orig>ma</orig><reg>m'à</reg></choice> fatto <choice><orig>Amor</orig><reg>amor</reg></choice> tremante &amp7; fioco.</l>
                      <l n="12">Et <choice><orig>ueggior</orig><reg>veggi' or</reg></choice> ben che caritate accesa</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Lega la lingua altrui&p2c; gli spirti <choice><orig><abbr>īuola</abbr><expan>inuola</expan></orig><reg>invola</reg></choice>&p2semicolon;</l>
                      <l n="14">Chi <choice><orig>po</orig><reg>pò</reg></choice> dir <choice><orig>comegli</orig><reg>com'egli</reg></choice> arde&p2c; <choice><orig>en</orig><reg>è 'n</reg></choice> piccol foco<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf171" type="sonnet" n="171"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#blue #fs24pt">G</hi><hi rendition="#small-caps">i</hi>unto <choice><orig>ma</orig><reg>m'à</reg></choice> <choice><orig>amor</orig><reg>Amor</reg></choice> fra belle &amp7; crude braccia&v2c;</l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">Che <choice><orig><abbr>mācidono</abbr><expan>mancidono</expan></orig><reg>m'ancidono</reg></choice> a torto&p2semicolon; &amp7; <choice><orig>sio</orig><reg>s'io</reg></choice> mi doglio&v2c;</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3"><choice><orig>Doppial</orig><reg>Doppia 'l</reg></choice> martir&p2semicolon; onde pur <choice><orig>comio</orig><reg>com'io</reg></choice> soglio&p2c;</l>
