   <text>
      <body>
         <pb n="charta 59 recto" facs="../images/vat-lat3195-f/vat-lat3195-f-059r.jpg" /> <!-- Change the number of the charta in both places "charta 1 verso" and -f-0001v.jpg", according to the charta to be encoded -->
          <lg type="sonnet" xml:id="rvf293" n="293"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><choice><orig><hi rendition="#red #fs24pt">S</hi><hi rendition="#small-caps">i</hi></orig><reg><hi rendition="#red #fs24pt">S</hi><hi rendition="#small-caps">’i</hi></reg></choice>o <choice><orig>auesse</orig><reg>avesse</reg></choice> pensato che <choice><orig>si</orig><reg>sì</reg></choice> care</l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">Fossin le <choice><orig>uoci</orig><reg>voci</reg></choice> <choice><orig>de</orig><reg>de’</reg></choice> <choice><orig>sospir</orig><reg>sospir’</reg></choice> miei in rima</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Fatte <choice><orig>laurei</orig><reg>l'avrei</reg></choice><supplied>,</supplied> dal sospirar mio prima<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="4">In numero <choice><orig>piu</orig><reg>più</reg></choice> spesse<choice><orig> <g ref="#virgsusp"/></orig><reg>,</reg></choice> in stil <choice><orig>piu</orig><reg>più</reg></choice> rare<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Morta colei che mi facea parlare<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="6">Et che si <choice><orig>staua</orig><reg>stava</reg></choice> <choice><orig>de</orig><reg>de’</reg></choice> <choice><orig>pensier</orig><reg>pensier’</reg></choice> miei in cima<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">&Non; posso<choice><orig> &virg-underdot;</orig><reg>,</reg></choice> &amp7; &non; <choice><orig>o</orig><reg>ò</reg></choice> <choice><orig>piu</orig><reg>più</reg></choice> <choice><orig>si</orig><reg>sì</reg></choice> dolce lima<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="8">Rime aspre &amp7; fosche far <choice><orig>soaui</orig><reg>soavi</reg></choice> &amp7; chiare<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><choice><orig><hi rendition="#sestet_maker">~</hi> </orig><reg></reg></choice>Et certo ogni mio studio &in; quel <choice><abbr>tēpo</abbr><expan>tempo</expan></choice> era</l>
                      <l n="10">Pur <choice><orig>disfogare</orig><reg>di sfogare</reg></choice> il doloroso core</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">In <choice><orig>qual che</orig><reg>qualche</reg></choice> modo<choice><orig> .</orig><reg>,</reg></choice> &non; <choice><orig>dacquistar</orig><reg>d'acquistar</reg></choice> fama<choice><orig> .</orig><reg>.</reg></choice></l>
                      <l n="12">Pianger cercai<choice><orig> .</orig><reg>,</reg></choice> &non; <choice><orig>gia</orig><reg>già</reg></choice> del <choice><abbr>piāto</abbr><expan>pianto</expan></choice> honore<choice><orig> .</orig><reg>;</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Or <choice><orig>uorrei</orig><reg>vorrei</reg></choice> ben piacer<choice><orig> .</orig><reg>,</reg></choice> ma <choice><abbr>&q-tilde;lla</abbr><expan>quella</expan></choice> altera</l>
                      <l n="14">Tacito stanco dopo <choice><orig>s<subst><del type="erasure"><gap reason="illegible"/></del><add>e</add></subst></orig><reg>sé</reg></choice> mi chiama<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf294" type="sonnet" n="294"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#blue #fs24pt">S</hi><hi rendition="#small-caps">o</hi>leasi nel mio cor star bella &amp7; <choice><orig>uiua</orig><reg>viva</reg></choice></l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
