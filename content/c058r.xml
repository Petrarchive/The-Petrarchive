   <text>
      <body>
         <pb n="charta 58 recto" facs="../images/vat-lat3195-f/vat-lat3195-f-058r.jpg" /> <!-- Change the number of the charta in both places "charta 1 verso" and -f-0001v.jpg", according to the charta to be encoded -->
          <lg type="sonnet" xml:id="rvf285" n="285"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><choice><orig><hi rendition="#red #fs24pt">N</hi><hi rendition="#small-caps">e</hi></orig><reg><hi rendition="#red #fs24pt">N</hi><hi rendition="#small-caps">é</hi></reg></choice> mai pietosa madre al caro figlio</l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2"><choice><orig>Ne</orig><reg>Né</reg></choice> donna accesa al suo sposo dilecto</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Die<supplied>’</supplied> &con; tanti sospir<supplied>’</supplied><choice><orig> &virg;</orig><reg>,</reg></choice> con tal sospetto</l>
                      <l n="4">In dubbio stato <choice><orig>si</orig><reg>sì</reg></choice> fedel consiglio<choice><orig>.</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Come a me quella <choice><orig>chel</orig><reg>che ’l</reg></choice> mio <choice><orig>graue</orig><reg>grave</reg></choice> exiglio</l>
                      <l n="6"><choice><abbr>Mirādo</abbr><expan>Mirando</expan></choice> dal suo eterno alto ricetto<choice><orig> <g ref="#virgsusp"/></orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Spesso a me torna <choice><orig>colusato</orig><reg>co l’usato</reg></choice> affecto<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="8">Et di doppia pietate ornata il ciglio<choice><orig></orig><reg>,</reg></choice></l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9">Or di madre<choice><orig> &virg;</orig><reg>,</reg></choice> or <choice><orig><abbr>damāte</abbr><expan>damante</expan></orig><reg>d’amante</reg></choice><choice><orig> .</orig><reg>,</reg></choice> or teme<choice><orig> .</orig><reg>,</reg></choice> or arde</l>
                      <l n="10"><choice><orig>Donesto</orig><reg>D’onesto</reg></choice> foco<choice><orig>.</orig><reg>;</reg></choice> &amp7; nel parlar mi mostra</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Quel <choice><orig>chen</orig><reg>che ’n</reg></choice> questo <choice><orig>uiaggio</orig><reg>viaggio</reg></choice> fugga o segua<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                      <l n="12">Contando i casi de la <choice><orig>uita</orig><reg>vita</reg></choice> nostra<supplied>,</supplied></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Pregando <choice><orig>cha</orig><reg>ch’ a</reg></choice> <choice><orig>leuar</orig><reg>levar</reg></choice> <choice><orig>lalma</orig><reg>l’alma</reg></choice> &non; tarde<choice><orig> .</orig><reg>;</reg></choice></l>
                      <l n="14">Et sol <choice><orig>quantella</orig><reg>quant’ ella</reg></choice> parla<choice><orig> &virg;</orig><reg></reg></choice> <choice><orig>o</orig><reg>ò</reg></choice> pace<choice><orig> &virg;</orig><reg></reg></choice> o tregua<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf286" type="sonnet" n="286"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#blue #fs24pt">S</hi><hi rendition="#small-caps">e</hi> <choice><orig>quellaura</orig><reg>quell’aura</reg></choice> <choice><orig>soaue</orig><reg>soave</reg></choice> de<supplied>’</supplied> sospiri</l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2"><choice><orig>Chi odo</orig><reg>Ch’i’ odo</reg></choice> di colei che qui fu mia</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3"><choice><abbr>Dōna</abbr><expan>Donna</expan></choice><choice><orig> &virg-underdot;</orig><reg>,</reg></choice> or <choice><orig>e</orig><reg>è</reg></choice> &in; cielo<choice><orig> .&amp7;</orig><reg>, &amp7;</reg></choice> anchor par qui sia<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="4">Et <choice><orig>uiua</orig><reg>viva</reg></choice> <choice><orig>.</orig><reg></reg></choice>&amp7; senta <choice><orig>.</orig><reg></reg></choice>&amp7; <choice><orig>uada</orig><reg>vada</reg></choice><choice><orig> .</orig><reg>,</reg></choice> &amp7; ami <choice><orig>.</orig><reg></reg></choice>&amp7; spiri<choice><orig> .</orig><reg>,</reg></choice></l>
