          <lg xml:id="rvf316" type="sonnet" n="316"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#fs24pt #blue">T</hi><hi rendition="#small-caps">e</hi>mpo era omai da <choice><orig>trouar</orig><reg>trovar</reg></choice> pace o triegua</l>
                      <l n="2">Di <choice><abbr>tāta</abbr><expan>tanta</expan></choice> guerra<choice><orig> .</orig><reg>, </reg></choice>&amp7; erane &in; <choice><orig>uia</orig><reg>via</reg></choice> forse<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Se non che <supplied>’</supplied> lieti passi indietro torse</l>
                      <l n="4">Chi le <choice><orig><abbr>disaguagliāçe</abbr><expan>disaguagliançe</expan></orig><reg>disaguaglianze</reg></choice> nostre adegua<choice><orig> </orig><reg></reg></choice>.</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5"><choice><orig>Che</orig><reg>Ché</reg></choice><supplied>,</supplied> come nebbia al <choice><orig>uento</orig><reg>vento</reg></choice> si dilegua<supplied>,</supplied></l>
                      <l n="6"><choice><orig>Cosi</orig><reg>Così</reg></choice> sua <choice><orig>uita</orig><reg>vita</reg></choice> subito trascorse</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Quella che <choice><orig>gia</orig><reg>già</reg></choice> <choice><orig>co</orig><reg>co’</reg></choice> <choice><orig>begliocchi</orig><reg>begli occhi</reg></choice> <choice><orig>miscorse</orig><reg>mi scorse</reg></choice><choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="8">Et or <choice><orig>conuen</orig><reg>conven</reg></choice> che col penser la segua<choice><orig> </orig><reg></reg></choice>.</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><choice><orig><hi rendition="#sestet_marker">~</hi></orig><reg></reg></choice> Poco <choice><orig>aueu&#7841;</orig><reg>aveva</reg></choice> <choice><orig>andugiar</orig><reg>a ’ndugiar</reg></choice><supplied>;</supplied> <choice><orig>che</orig><reg>ché</reg></choice> <choice><orig><abbr>gliāni</abbr><expan>glianni</expan></orig><reg>gli anni</reg></choice> <choice><orig>el</orig><reg>e ’l</reg></choice> pelo</l><!-- check a with dot below -->
                      <l n="10"><choice><orig><abbr>Cāgiauano</abbr><expan>Cangiauano</expan></orig><reg>Cangiavano</reg></choice> i costumi<choice><orig> .</orig><reg>,</reg></choice> onde sospetto</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Non fora il ragionar del mio mal seco<choice><orig> </orig><reg></reg></choice>.</l>
                      <l n="12">&Con; che honesti sospiri <choice><orig>  <subst><del type="erasure"><gap reason="illegible"/></del><add>l</add></subst>aurei</orig><reg>l’avrei</reg></choice> detto</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
