      <body>
          <pb xml:id="c061v" n="charta 61 verso" facs="../images/vat-lat3195-f/vat-lat3195-f-061v.jpg" />
          <lg type="sonnet" xml:id="rvf313" n="313"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#red #fs24pt">P</hi><hi rendition="#small-caps">a</hi>ssato <choice><orig>el</orig><reg>è ’l</reg></choice> tempo omai<choice><orig> &virg;</orig><reg>,</reg></choice> lasso<choice><orig> &virg;</orig><reg>,</reg></choice> che tanto</l>
                      <l n="2">Con r<subst><del type="erasure"><gap reason="illegible"/></del><add>e</add></subst>fr<subst><del type="erasure"><gap reason="illegible"/></del><add>i</add></subst>gerio in <choice><orig>meççol</orig><reg>mezzo ’l</reg></choice> foco <choice><orig>uissi</orig><reg>vissi</reg><choice><orig> </orig><reg></reg></choice></choice>.</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Passato <choice><orig>e</orig><reg>è</reg></choice> quella di <choice><orig>chio</orig><reg>ch’io</reg></choice> piansi &amp7; scrissi<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="4">Ma lasciato <choice><orig>ma</orig><reg>m’à</reg></choice> ben la <choice><abbr>pēna</abbr><expan>penna</expan></choice> <choice><orig>el</orig><reg>e ’l</reg></choice> <choice><abbr>piāto</abbr><expan>pianto</expan></choice><choice><orig> </orig><reg></reg></choice>.</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Passato <choice><orig>el</orig><reg>è ’l</reg></choice> <choice><orig>uiso</orig><reg>viso</reg></choice> <choice><orig>si</orig><reg>sì</reg></choice> leggiadro &amp7; santo<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="6">Ma passando i dolci occhi al cor <choice><orig>ma</orig><reg>m’à</reg></choice> fissi<choice><orig> .</orig><reg>;</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Al cor <choice><orig>gia</orig><reg>già</reg></choice> mio<choice><orig> .</orig><reg>,</reg></choice> che seguendo partissi</l>
                      <l n="8">Lei <choice><orig>chauolto</orig><reg>ch’avolto</reg></choice> <choice><orig>lauea</orig><reg>l’avea</reg></choice> nel suo bel manto<choice><orig> </orig><reg></reg></choice>.</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><choice><orig><hi rendition="#sestet_marker">~</hi></orig><reg></reg></choice> <choice><orig>Ellalsene</orig><reg>Ella ’l se ne</reg></choice> <choice><orig>porto</orig><reg>portò</reg></choice> sotterra<choice><orig> .</orig><reg></reg></choice> <choice><orig>en</orig><reg>e ’n</reg></choice> cielo</l><!-- check sestet markers all sonnets -->
                      <l n="10"><choice><orig>Oue</orig><reg>Ove</reg></choice> or triumpha<choice><orig> &virg;</orig><reg>,</reg></choice> ornata <choice><orig>delalloro</orig><reg>de l’alloro</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Che <choice><orig>merito</orig><reg>meritò</reg></choice> la sua <choice><orig>inuicta</orig><reg>invicta</reg></choice> honestate<supplied>.</supplied></l>
                      <l n="12"><choice><orig>Cosi</orig><reg>Così</reg></choice> disciolto dal mortal mio <choice><orig>uelo</orig><reg>velo</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13"><choice><orig>Cha</orig><reg>Ch’a</reg></choice> <choice><orig>força</orig><reg>forza</reg></choice> mi tien qui<choice><orig> .</orig><reg>,</reg></choice> <choice><orig>fossio</orig><reg>foss’io</reg></choice> con loro</l>
                      <l n="14">Fuor <choice><orig>de</orig><reg>de’</reg></choice> sospir fra <choice><orig>lanime</orig><reg>l’anime</reg></choice> beate<choice><orig> </orig><reg></reg></choice>.</l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf314" type="sonnet" n="314"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#blue #fs24pt">M</hi><hi rendition="#small-caps">e</hi>nte mia<supplied>,</supplied> che presaga <choice><orig>de</orig><reg>de’</reg></choice> tuoi damni<choice><orig> .</orig><reg>,</reg></choice></l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2">Al <choice><abbr>tēpo</abbr><expan>tempo</expan></choice> liet<subst><del type="erasure"><gap reason="illegible"/></del><add>o</add></subst> <choice><orig>gia</orig><reg>già</reg></choice> pensosa &amp7; trista<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3"><choice><orig>Sintentamente</orig><reg>Sì ’ntentamente</reg></choice> <choice><orig>nelamata</orig><reg>ne l’amata</reg></choice> <choice><orig>uista</orig><reg>vista</reg></choice></l>
                      <l n="4">Requie <choice><orig>cercaui</orig><reg>cercavi</reg></choice> <choice><orig>de</orig><reg>de’</reg></choice> futuri affanni<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5"><choice><orig>A gliatti</orig><reg>Agli atti</reg></choice><choice><orig> &virg;</orig><reg>,</reg></choice> a le parole<choice><orig> &virg;</orig><reg>,</reg></choice> al <choice><orig>uiso</orig><reg>viso</reg></choice><choice><orig> &virg;</orig><reg>,</reg></choice> ai panni<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                      <l n="6">A la <choice><orig>noua</orig><reg>nova</reg></choice> <choice><orig>pieta</orig><reg>pietà</reg></choice> con dolor mista<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7">Potei ben dir<choice><orig> &virg;</orig><reg>,</reg></choice> se del tutto eri <choice><orig>auista</orig><reg>avista</reg></choice><choice><orig> &virg;</orig><reg>:</reg></choice></l>
                      <l n="8">Questo <choice><orig>e</orig><reg>è</reg></choice> <choice><orig>lultimo</orig><reg>l’ultimo</reg></choice> <choice><orig>di</orig><reg>dì</reg></choice> <choice><orig>de</orig><reg>de’</reg></choice> miei dolci <choice><abbr>āni</abbr><expan>anni</expan></choice><choice><orig> </orig><reg></reg></choice>.</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><choice><orig><hi rendition="#sestet_marker">~</hi></orig><reg></reg></choice> Qual <choice><orig>dolceçça</orig><reg>dolcezza</reg></choice> fu quella<supplied>,</supplied> o misera alma<choice><orig> </orig><reg></reg></choice>.</l>
                      <l n="10">Come <choice><orig>ardauamo</orig><reg>ardavamo</reg></choice> in quel <choice><abbr>pūto</abbr><expan>punto</expan></choice> <choice><orig>chi</orig><reg>ch’i’</reg></choice> <choice><orig>uidj</orig><reg>vidi</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11"><choice><orig>Gliocchi</orig><reg>Gli occhi</reg></choice><supplied>,</supplied> <choice><orig>iquai</orig><reg>i quai</reg></choice> &non; <choice><orig>deuea</orig><reg>devea</reg></choice> <choice><orig>riueder</orig><reg>riveder</reg></choice> mai<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="12">&Quando; a lor come <choice><orig>a</orig><reg>a’</reg></choice> duo amici <choice><orig>piu</orig><reg>più</reg></choice> fidi<supplied>,</supplied></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13">Partendo &in; guardia la <choice><orig>piu</orig><reg>più</reg></choice> nobil salma<supplied>,</supplied></l>
                      <l n="14">I miei cari penseri <choice><orig>&#x007c;</orig><reg></reg></choice> <choice><orig>el</orig><reg>e ’l</reg></choice> cor lasciai<choice><orig> </orig><reg></reg></choice>.</l><!-- check vertical bar, also 316, 13 -->
                  </lg>
