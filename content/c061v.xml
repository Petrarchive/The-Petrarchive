                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><choice><orig><hi rendition="#red #fs24pt">T</hi><hi rendition="#small-caps">v</hi>tta</orig><reg><hi rendition="#red #fs24pt">T</hi><hi rendition="#small-caps">u</hi>tta</reg></choice> la mia fiorita &amp7; <choice><orig>uerde</orig><reg>verde</reg></choice> etade</l>
                      <l n="2"><choice><orig>Passaua</orig><reg>Passava</reg></choice><choice><orig> .</orig><reg>,</reg></choice> <choice><orig>entepidir</orig><reg>e ’ntepidir</reg></choice> sentia <choice><orig>gial</orig><reg>già ’l</reg></choice> foco</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3"><choice><orig>Charse</orig><reg>Ch’arse</reg></choice> il mio core<choice><orig> .</orig><reg>,</reg></choice> &amp7; era <choice><abbr>giūto</abbr><expan>giunto</expan></choice> al loco &virg;</l>
                      <l n="4"><choice><orig>Oue</orig><reg>Ove</reg></choice> scende la <choice><orig>uita</orig><reg>vita</reg></choice> <choice><orig>chal</orig><reg>ch’al</reg></choice> fin cade<choice><orig> </orig><reg></reg></choice>.</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5"><choice><orig>Gia</orig><reg>Già</reg></choice> <choice><orig><abbr>incōminciaua</abbr><expan>incomminciaua</expan></orig><reg>incominciava</reg></choice> a prender securtade</l>
                      <l n="6">La mia cara nemica a poco a poco</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7"><choice><orig>De</orig><reg>De’</reg></choice> suoi sospetti<choice><orig> .</orig><reg></reg></choice> &amp7; <choice><orig>riuolgeua</orig><reg>rivolgeva</reg></choice> in gioco</l>
                      <l n="8">Mie pene acerbe sua dolce honestade<choice><orig> </orig><reg></reg></choice>.</l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><choice><orig><hi rendition="#sestet_marker">~</hi></orig><reg></reg></choice> Presso <choice><orig>eral</orig><reg>era ’l</reg></choice> tempo <choice><orig>doue</orig><reg>dove</reg></choice> amor si <choice><abbr>scōtra</abbr><expan>scontra</expan></choice></l>
                      <l n="10">&Con; castitate<choice><orig> .</orig><reg>, </reg></choice>&amp7; <choice><orig>a gliamanti</orig><reg>agli amanti</reg></choice> <choice><orig>e</orig><reg>è</reg></choice> dato</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Sedersi inseme <choice><orig>.</orig><reg></reg></choice>&amp7; dir che lor incontra<choice><orig> </orig><reg></reg></choice>.</l>
                      <l n="12">Morte ebbe <choice><orig>inuidia</orig><reg>invidia</reg></choice> al mio felice stato<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13"><choice><orig>Ançi</orig><reg>Anzi</reg></choice> a la speme<choice><orig> .</orig><reg>;</reg></choice> &amp7; feglisi <choice><orig><abbr>alincōtra</abbr><expan>alincontra</expan></orig><reg>a l’incontra</reg></choice></l>
                      <l n="14">A <choice><orig>meçça</orig><reg>mezza</reg></choice> <choice><orig>uia</orig><reg>via</reg></choice> come nemico a<subst><del type="erasure"><gap reason="illegible"/></del><add>rmato</add></subst><choice><orig> </orig><reg></reg></choice>.</l>
                  </lg>
