      <body>
         <pb n="charta 37 recto" facs="../images/vat-lat3195-f/vat-lat3195-f-037r.jpg" /> 
          <lg type="sonnet" xml:id="rvf178" n="178"> 
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#red #fs24pt">A</hi><hi rendition="#small-caps">m</hi>or mi sprona in un <choice><abbr>tēpo</abbr><expan>tempo</expan></choice><choice><orig> .</orig><reg></reg></choice> &amp7; affrena<choice><orig> .</orig><reg>,</reg></choice></l> 
                      <l n="2">Assecura<choice><orig> &virg;</orig><reg></reg></choice> &amp7; <choice><orig>spauenta</orig><reg>spaventa</reg></choice><choice><orig> .</orig><reg>,</reg></choice> arde<choice><orig> .</orig><reg></reg></choice> &amp7; agghiaccia<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="3">Gradisce &amp7; sdegna<choice><orig> .</orig><reg>,</reg></choice> a <choice><orig>s<subst><del type="erasure"><gap reason="illegible"/></del><add>e</add></subst></orig><reg>sé</reg></choice> mi chiama &amp7; scaccia<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="4">Or mi tene in <choice><orig>sperança</orig><reg>speranza</reg></choice><choice><orig> .</orig><reg></reg></choice> &amp7; or &in; pena<choice><orig> .</orig><reg>,</reg></choice></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="5">Or alto<choice><orig> &virg;</orig><reg></reg></choice> or basso il meo cor lasso mena<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="6"><choice><orig>Ondel</orig><reg>Onde ’l </reg></choice> <choice><orig>uago</orig><reg>vago</reg></choice> desir perde la traccia</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="7"><choice><orig>El<reg>E ’l</reg></orig></choice> suo <choice><abbr>sōmo</abbr><expan>sommo</expan></choice> piacer par che li spiaccia<choice><orig> .</orig><reg>,</reg></choice></l>
                      <l n="8"><choice><orig>Derrore</orig><reg>D’errore</reg></choice> &si; <choice><orig>nouo</orig><reg>novo</reg></choice> la mia mente &e; piena<choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
              <lg type="sestet">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="9"><hi rendition="#sestet_marker">~</hi> <choice><orig>Vn</orig><reg>Un</reg></choice> amico penser le mostra il <choice><orig>uado</orig><reg>vado</reg></choice></l>
                      <l n="10">Non <choice><orig>dacqua</orig><reg>d’acqua</reg></choice><choice><orig> &virg;</orig><reg></reg></choice> che &per; gli occhi si <choice><orig>resolua</orig><reg>resolva</reg></choice><supplied>,</supplied></l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="11">Da gir tosto<choice><orig> &virg;</orig><reg></reg></choice> <choice><orig>oue</orig><reg>ove</reg></choice> spera esser <choice><abbr>contēta</abbr><expan>contenta</expan></choice>&p2semicolon;</l>
                      <l n="12">Poi quasi maggior <choice><orig>força</orig><reg>forza</reg></choice> indi la <choice><orig>suolua</orig><reg>svolva</reg></choice>&v2c;</l>
                  </lg>
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="13"><choice><orig>Conuen</orig><reg>Conven</reg></choice> <choice><orig>chaltra</orig><reg>ch’altra</reg></choice> <choice><orig>uia</orig><reg>via</reg></choice> segua&p2c; &amp7; mal suo grado</l>
                      <l n="14">A sua lunga&v2c; &amp7; mia<supplied>,</supplied> morte <choice><abbr>cōsenta</abbr><expan>consenta</expan></choice><choice><orig> .</orig><reg>.</reg></choice></l>
                  </lg>
              </lg>
          </lg>
          <space corresp="#canvasline"/>
          <lg xml:id="rvf179" type="sonnet" n="179"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
              <lg type="octave">
                  <lg type="dblvrs" corresp="#canvasline">
                      <l n="1"><hi rendition="#blue #fs24pt">G</hi><hi rendition="#small-caps">e</hi>ri<supplied>,</supplied> quando talor meco sadira </l> <!-- Change initial color (red or blue) according to the initiasl of the sonnet to be encoded -->
                      <l n="2"><choice><orig>Lamia</orig><reg>La mia</reg></choice> dolce nemica&v2c; <choice><orig>che</orig><reg>ch’è</reg></choice> &si; altera<choice><orig> .</orig><reg>,</reg></choice></l>
