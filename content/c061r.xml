         <lg xml:id="rvf312" type="sonnet" n="312"> <!-- Change the sonnet xml:id and number according to the sonnet to be encoded -->
             <lg type="octave">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="1"><choice><orig><hi rendition="#fs24pt #blue">N</hi><hi rendition="#small-caps">e</hi></orig><reg><hi rendition="#fs24pt #blue">N</hi><hi rendition="#small-caps">é</hi></reg></choice><choice><orig>&per;</orig><reg> per</reg></choice> sereno ciel ir <choice><orig>uaghe</orig><reg>vaghe</reg></choice> stelle<choice><orig> .</orig><reg>,</reg></choice></l>
                     <l n="2"><choice><orig>Ne</orig><reg>Né</reg></choice> &per; tranquillo mar legni spalmati<choice><orig>.</orig><reg>,</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="3"><choice><orig>Ne</orig><reg>Né</reg></choice> &per; campagne <choice><orig>caualieri</orig><reg>cavalieri</reg></choice> armati<choice><orig> .</orig><reg>,</reg></choice></l>
                     <l n="4"><choice><orig>Ne</orig><reg>Né</reg></choice> &per; bei boschi allegre fere &amp7; snelle.</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="5"><choice><orig>Ne</orig><reg>Né</reg></choice> <choice><orig>daspettato</orig><reg>d’aspettato</reg></choice> ben fresche <choice><orig>nouelle</orig><reg>novelle</reg></choice><choice><orig>.</orig><reg>,</reg></choice></l>
                     <l n="6"><choice><orig>Ne</orig><reg>Né</reg></choice> dir <choice><orig>damore</orig><reg>d’amore</reg></choice> in stili alti &amp7; orna<subst><del type="erasure"><gap reason="illegible"/></del><add>ti</add></subst><supplied>,</supplied></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="7"><choice><orig>Ne</orig><reg>Né</reg></choice> tra chiare fontane <choice><orig>.</orig><reg></reg></choice>&amp7; <choice><orig>uerdi</orig><reg>verdi</reg></choice> prati</l>
                     <l n="8">Dolce cantare honeste donne &amp7; belle<choice><orig> .</orig><reg>.</reg></choice></l>
                 </lg>
             </lg>
             <lg type="sestet">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="9"><choice><orig><hi rendition="#sestet_marker">~</hi>Ne</orig><reg>Né</reg></choice> altro <choice><orig>sara</orig><reg>sarà</reg></choice> mai <choice><orig>chal</orig><reg>ch’al</reg></choice> cor <choice><orig><abbr>maggiūga</abbr><expan>maggiunga</expan></orig><reg>m’aggiunga</reg></choice><choice><orig> .</orig><reg>,</reg></choice></l>
                     <l n="10"><choice><orig>Si</orig><reg>Sì</reg></choice> seco il seppe quella sepellire</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="11">Che sola <choice><orig>a gliocchi</orig><reg>agli occhi</reg></choice> miei fu lume &amp7; speglio<choice><orig> .</orig><reg>.</reg></choice></l>
                     <l n="12">Noia <choice><orig>mel</orig><reg>m’è ’l</reg></choice> <choice><orig>uiuer</orig><reg>viver</reg></choice> <choice><orig>si</orig><reg>sì</reg></choice> <choice><orig>grauosa</orig><reg>gravosa</reg></choice> &amp7; lunga</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="13"><choice><orig>Chi</orig><reg>Ch’i’</reg></choice> chiamo il fine<choice><orig> .</orig><reg></reg></choice> &per; lo gran desire</l>
                     <l n="14">Di <choice><orig>riueder</orig><reg>riveder</reg></choice><choice><orig> &virg;</orig><reg></reg></choice> cui non <choice><orig>ueder</orig><reg>veder</reg></choice> <choice><orig>ful</orig><reg>fu ’l</reg></choice> meglio<choice><orig> .</orig><reg>.</reg></choice></l>
                 </lg>
             </lg>
