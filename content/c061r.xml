<text>
     <body>
        <pb n="charta 61 recto" facs="../images/vat-lat3195-f/vat-lat3195-f-061r.jpg" />
         <lg type="sonnet" xml:id="rvf309" n="309">
             <lg type="octave">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="1"><choice><orig><hi rendition="#red #fs24pt">L</hi><hi rendition="#small-caps">a</hi>lto</orig><reg>L’alto</reg></choice> &amp7; <choice><orig>nouo</orig><reg>novo</reg></choice> miracol <choice><orig>cha</orig><reg>ch’a’</reg></choice> <choice><orig>di</orig><reg>dì</reg></choice> nostri</l>
                     <l n="2"><choice><orig>Apparue</orig><reg>Apparve</reg></choice> al mondo<choice><orig> .</orig><reg></reg></choice> &amp7; star seco non <choice><orig>uolse</orig><reg>volse</reg></choice><supplied>,</supplied></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="3">Che sol ne <choice><orig>mostrol</orig><reg>mostrò ’l</reg></choice> ciel<choice><orig><subst><del type="erasure"><gap reason="illegible"/></del><add> &virg-underdot;</add></subst></orig><reg>,</reg></choice> poi sel ritolse<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                     <l n="4">Per adornarne i suoi stellanti chiostri<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="5">Vuol <choice><orig>chi</orig><reg>ch’i’</reg></choice> depinga a chi nol <choice><orig>uide.</orig><reg>vide,</reg></choice> <choice><orig>el</orig><reg>e ’l</reg></choice> mostri<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                     <l n="6">Amor<choice><orig>.</orig><reg>,</reg></choice> <choice><orig><abbr>chē</abbr><expan>chen</expan></orig><reg>che ’n</reg></choice> prima la mia lingua sciolse<choice><orig> .</orig><reg>,</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="7">Poi mille <choice><orig>uolte</orig><reg>volte</reg></choice> indarno a <choice><orig>lopra</orig><reg>l’opra</reg></choice> <choice><orig>uolse</orig><reg>volse</reg></choice></l>
                     <l n="8">Ingegno<choice><orig> &virg;</orig><reg>,</reg></choice> tempo<choice><orig> &virg;</orig><reg>,</reg></choice> <choice><abbr>pēne</abbr><expan>penne</expan></choice><choice><orig> &virg;</orig><reg>,</reg></choice> carte<choice><orig> &virg;</orig><reg>,</reg></choice> <choice><orig>enchiostri</orig><reg>e ’nchiostri</reg></choice><choice><orig> .</orig><reg>.</reg></choice></l><!-- check footnote for enchiostri -->
                 </lg>
             </lg>
             <lg type="sestet">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="9"><hi rendition="#sestet_marker">~</hi>Non son al <choice><abbr>sōmo</abbr><expan>sommo</expan></choice> anchor giunte le rime<supplied>,</supplied></l><!-- check sestet markers -->
                     <l n="10">In me il conosco<supplied>,</supplied> &amp7; <choice><orig>proual</orig><reg>proval</reg></choice> ben <choice><abbr>chiūque</abbr><expan>chiunque</expan></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="11"><choice><orig>En</orig><reg>È ’n</reg></choice> fin a qui che <choice><orig>damor</orig><reg>d’amor</reg></choice> parli<choice><orig> &virg;</orig><reg></reg></choice> o <choice><orig>scriua</orig><reg>scriva</reg></choice><choice><orig> .</orig><reg>.</reg></choice></l>
                     <l n="12">Chi sa pensare<choice><orig> &virg;</orig><reg>,</reg></choice> il <choice><orig>uer</orig><reg>ver</reg></choice> tacito estime<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="13"><choice><orig>Chogni</orig><reg>Ch’ogni</reg></choice> stil <choice><orig>uince</orig><reg>vince</reg></choice><choice><orig> .</orig><reg>,</reg></choice> &amp7; poi sospire<choice><orig> .</orig><reg>:</reg></choice> <choice><orig><abbr>adūque</abbr><expan>adunque</expan></orig><reg>Adunque</reg></choice></l>
                     <l n="14">Beati <choice><orig>gliocchi</orig><reg>gli occhi</reg></choice><choice><orig> &virg;</orig><reg></reg></choice> che la <choice><orig>uider</orig><reg>vider</reg></choice> <choice><orig>uiua</orig><reg>viva</reg></choice><choice><orig> .</orig><reg>.</reg></choice></l>
                 </lg>
             </lg>
         </lg>
         <space corresp="#canvasline"/>
         <lg xml:id="rvf310" type="sonnet" n="310">
             <lg type="octave">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="1"><hi rendition="#blue #fs24pt">Ç</hi><hi rendition="#small-caps">e</hi>phiro torna<choice><orig> .</orig><reg>,</reg></choice> <choice><orig>el</orig><reg>e ’l</reg></choice> bel tempo rimena</l> <!-- HTML capital visigoth Z is &#xA762; I have left ç because it looks closer to the manuscript (Paolo)-->
                     <l n="2"><choice><orig>Ei</orig><reg>E i</reg></choice> fiori<choice><orig> &virg;</orig><reg></reg></choice> &amp7; <choice><orig>lerbe</orig><reg>l’erbe</reg></choice><choice><orig> .</orig><reg>,</reg></choice> sua dolce famiglia<choice><orig> .</orig><reg>,</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="3">Et garrir <choice><orig>progne</orig><reg>Progne</reg></choice> <choice><orig>&virg;&#x0312;</orig><reg></reg></choice> &amp; pianger <choice><orig>philomena</orig><reg>Philomena</reg></choice><choice><orig> .</orig><reg>,</reg></choice></l>
                     <l n="4">Et <choice><orig>prima=uera</orig><reg>primavera</reg></choice> candida &amp7; <choice><orig>uermiglia</orig><reg>vermiglia</reg></choice><choice><orig> .</orig><reg>.</reg></choice></l><!-- check word separator -->
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="5">Ridono i prati<choice><orig> .</orig><reg></reg></choice> <choice><orig>el</orig><reg>e ’l</reg></choice> ciel si rasserena<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                     <l n="6"><choice><orig>Gioue</orig><reg>Giove</reg></choice> <choice><orig>sallegra</orig><reg>s’allegra</reg></choice> di mirar sua figlia<choice><orig> .</orig><reg>;</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="7"><choice><orig>Laria</orig><reg>L’aria</reg></choice> &amp7; <choice><orig>lacqua</orig><reg>l’acqua</reg></choice> &amp7; la terra <choice><orig>e</orig><reg>è</reg></choice> <choice><orig>damor</orig><reg>d’amor</reg></choice> piena<choice><orig> .</orig><reg>;</reg></choice></l>
                     <l n="8">Ogni animal <choice><orig>damar</orig><reg>d’amar</reg></choice> si riconsiglia<supplied>.</supplied></l>
                 </lg>
             </lg>
             <lg type="sestet">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="9"><hi rendition="#sestet_marker">~</hi>Ma &per; me<supplied>,</supplied> lasso<supplied>,</supplied> tornano i <choice><orig>piu</orig><reg>più</reg></choice> <choice><orig>graui</orig><reg>gravi</reg></choice></l><!-- check all sestet markers -->
                     <l n="10">Sospiri<choice><orig> .</orig><reg>,</reg></choice> che del cor profondo tragge</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="11">Quella <choice><orig>chal</orig><reg>ch’al</reg></choice> ciel <choice><orig>sene</orig><reg>se ne</reg></choice> <choice><orig>porto</orig><reg>portò</reg></choice> le <choice><orig>chiaui</orig><reg>chiavi</reg></choice><choice><orig> .</orig><reg>;</reg></choice></l>
                     <l n="12">Et cantar augelletti<choice><orig> .</orig><reg></reg></choice> &amp7; fiorir piagge<choice><orig> .</orig><reg></reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="13"><choice><orig>En</orig><reg>E ’n</reg></choice> belle donne honeste atti <choice><orig>soaui</orig><reg>soavi</reg></choice></l>
                     <l n="14">Sono un deserto<choice><orig> .</orig><reg>,</reg></choice>&amp7; fere aspre <choice><orig>&virg;</orig><reg></reg></choice>&amp7; <choice><orig>seluagge</orig><reg>selvagge</reg></choice><choice><orig> .</orig><reg>.</reg></choice></l>
                 </lg>
             </lg>
         </lg>
         <space corresp="#canvasline"/>
         <lg xml:id="rvf311" type="sonnet" n="311">
             <lg type="octave">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="1"><choice><orig>Q<hi rendition="#red #fs24pt"></hi><hi rendition="#small-caps">v</hi>el</orig><reg>Quel</reg></choice> <choice><orig>rosignuol&#7885;</orig><reg>rosignuolo</reg></choice><choice><orig> &virg;</orig><reg>,</reg></choice> che <choice><orig>si</orig><reg>sì</reg></choice> <choice><orig>soaue</orig><reg>soave</reg></choice> piagne</l> <!-- check o with dot below -->
                     <l n="2">Forse suoi figli<choice><orig> .</orig><reg>,</reg></choice> o sua cara consorte<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="3">Di dolce&#xa763;&#xa763;a empie il cielo &amp7; le <choice><abbr>cāpagne</abbr><expan>campagne</expan></choice></l>
                     <l n="4">Con tante note <choice><orig>si</orig><reg>sì</reg></choice> pietose &amp7; scorte<choice><orig> .</orig><reg>,</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="5">Et tutta notte par che <choice><orig><abbr>maccōpagne</abbr><expan>maccompagne</expan></orig><reg>m’accompagne</reg></choice><choice><orig> .</orig><reg>,</reg></choice></l>
                     <l n="6">Et mi <choice><abbr>rāmente</abbr><expan>rammente</expan></choice> la mia dura sorte<choice><orig> .</orig><reg>;</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="7"><choice><orig>Chaltri</orig><reg>Ch’altri</reg></choice> che me non <choice><orig>o</orig><reg>ò</reg></choice> di <choice><orig>chi</orig><reg>ch’i’</reg></choice> mi lagne<choice><orig> &virg;</orig><reg>,</reg></choice></l>
                     <l n="8"><choice><orig>Chen</orig><reg>Ché ’n</reg></choice> dee non <choice><orig>credeuio</orig><reg>credev’io</reg></choice> regnasse <choice><orig>morte</orig><reg>Morte</reg></choice><choice><orig> .</orig><reg>.</reg></choice></l>
                 </lg>
             </lg>
             <lg type="sestet">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="9"><hi rendition="#sestet_marker">~</hi>O che <choice><orig>lieue</orig><reg>lieve</reg></choice> <choice><orig>e</orig><reg>è</reg></choice> inganar chi <choice><orig>sassecura</orig><reg>s’assecura</reg></choice><choice><orig> &#xF160;</orig><reg>!</reg></choice></l><!-- check punctuation -->
                     <l n="10"><choice><orig>Que</orig><reg>Que’</reg></choice> duo bei lumi assai <choice><orig>piu</orig><reg>più</reg></choice> <choice><orig>chel</orig><reg>che ’l</reg></choice> sol chiari</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="11">Chi <choice><orig>penso</orig><reg>pensò</reg></choice> mai <choice><orig>ueder</orig><reg>veder</reg></choice> far terra oscura<choice><orig> &#xF160;</orig><reg>?</reg></choice></l>
                     <l n="12">Or cognosco io<choice><orig> &virg;</orig><reg></reg></choice> che mia fera <choice><orig>uentura</orig><reg>ventura</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="13">Vuol che <choice><orig>uiuendo</orig><reg>vivendo</reg></choice> &amp7; lagrimando <choice><abbr>īpari</abbr><expan>impari</expan></choice><choice><orig> &#xF1F4;</orig><reg></reg></choice></l><!-- check punctuation -->
                     <l n="14">Come nulla qua <choice><orig>gi<subst><del type="erasure"><gap reason="illegible"/></del><add>u</add></subst></orig><reg>giù</reg></choice> diletta <choice><orig>&#11805;</orig><reg></reg></choice>&amp7; dura<choice><orig> .</orig><reg>.</reg></choice></l><!-- check punctuation -->
                 </lg>
             </lg>
         </lg>
